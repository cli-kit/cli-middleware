{
  "name": "cli-middleware",
  "version": "1.0.2",
  "description": "Middleware execution for the command module",
  "author": "muji <noop@xpm.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cli-kit/cli-middleware"
  },
  "bugs": {
    "url": "https://github.com/cli-kit/cli-middleware/issues",
    "email": "muji <noop@xpm.io>"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/cli-kit/cli-middleware/blob/master/LICENSE"
    }
  ],
  "keywords": [
    "cli",
    "middleware"
  ],
  "dependencies": {
    "cli-util": "~1.1.27"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "istanbul": "~0.4.2",
    "mocha": "~2.4.5",
    "coveralls": "~2.11.6"
  },
  "engine": [
    "node >= 0.10.21"
  ],
  "scripts": {
    "lint": "jshint . && jscs .",
    "readme": "mdp --force -v",
    "test": "mocha ${SPEC:-test/spec}",
    "cover": "istanbul cover _mocha -- ${SPEC:-test/spec}",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "mdp": {
    "title": "Middleware",
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/cli-kit/cli-middleware",
    "partial": [
      {
        "inc": [
          "introduction.md",
          "install.md"
        ]
      },
      {
        "inc": [
          "license.md"
        ]
      }
    ]
  }
}
